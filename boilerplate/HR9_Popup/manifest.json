{
    "manifest_version": 2,
    "name": "Self Care :)",
    "version": "0.1",
    "browser_action": {
        "default_title": "Sample Extension",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["timer.js"],
        "persistent": true
    },
    "permissions": [
        "activeTab"
    ]
}